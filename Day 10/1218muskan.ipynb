    "#Day 10\n",
    "\n",
    "a,b=input(\"enter two numbers: \").split()\n",
    "a=int(a)\n",
    "b=int(b)\n",
    "minm=min(a,b)\n",
    "\n",
    "for i in range (minm,0,-1):\n",
    "    if(a%i==0 and b%i==0):\n",
    "        print(\"HCF of\",a,\"and\",b,\"is\",i)\n",
    "        break;\n"
