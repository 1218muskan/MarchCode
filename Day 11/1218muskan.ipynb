    "## Day 11\n",
    "\n",
    "a,b=input(\"enter two numbers: \").split()\n",
    "a=int(a)\n",
    "b=int(b)\n",
    "maxm=max(a,b)\n",
    "\n",
    "for i in range(maxm,a*b+1):\n",
    "    if(i%a==0 and i%b==0):\n",
    "        print(\"LCM of\",a,'and',b,'is:',i)\n",
    "        break"
