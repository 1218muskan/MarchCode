    "## Day 9\n",
    "\n",
    "n=int(input(\"enter a number: \"))\n",
    "sum=n%10      # to extract last digit of number\n",
    "\n",
    "while n!=0:\n",
    "    r=n%10\n",
    "    n=n//10\n",
    "\n",
    "sum=sum+r\n",
    "\n",
    "print(\"sum of first and last digit of number is\",sum)"
